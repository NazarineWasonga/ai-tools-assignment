import spacy
nlp = spacy.load("en_core_web_sm")

text = "I bought the Samsung Galaxy S23 Ultra from Amazon and the camera is outstanding!"
doc = nlp(text)

print("Named Entities:")
for ent in doc.ents:
    print(ent.text, ent.label_)

sentiment = "positive" if "outstanding" in text or "great" in text else "negative"
print("\nSentiment:", sentiment)
